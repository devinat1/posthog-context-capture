{
  "name": "posthog-context-capture",
  "version": "1.0.1",
  "description": "CLI and MCP server to look up PostHog persons and their events",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "posthog-lookup": "./dist/index.js",
    "posthog-mcp": "./dist/mcp-server.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/mcp-server.ts --format esm --dts --clean",
    "typecheck": "tsc --noEmit",
    "dev": "tsx src/index.ts",
    "mcp": "tsx src/mcp-server.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "posthog",
    "mcp",
    "model-context-protocol",
    "claude",
    "analytics",
    "debugging"
  ],
  "author": "Devin Ersoy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/devinat1/posthog-context-capture"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
